{
  "compilerOptions": {
    "skipLibCheck": true,
    "lib": ["es2015"],
    "target": "es2019",
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "rewriteRelativeImportExtensions": true,
    "erasableSyntaxOnly": true,
    "verbatimModuleSyntax": true,
    "downlevelIteration": true,
    "allowJs": true
  }
}
