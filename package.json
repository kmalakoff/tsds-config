{
  "name": "tsds-config",
  "version": "0.1.1",
  "description": "Typescript dev stack shared configuration files: tsconfig.json, biome.json, etc",
  "keywords": [
    "tsds",
    "config",
    "configuration",
    "biome",
    "typescript",
    "tsconfig"
  ],
  "homepage": "https://github.com/kmalakoff/tsds-config",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/tsds-config.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./biome.json": "./biome.json",
    "./tsconfig.json": "./tsconfig.json",
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "source": "src/index.ts",
  "types": "dist/cjs/index.d.cts",
  "files": [
    "dist",
    "biome.json",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "biome check --write --unsafe",
    "prepublishOnly": "tsds validate",
    "test": "tsds test:node --no-timeouts",
    "version": "tsds version"
  },
  "devDependencies": {
    "@biomejs/biome": "*",
    "@types/mocha": "*",
    "@types/node": "*"
  },
  "engines": {
    "node": ">=0.8"
  }
}
